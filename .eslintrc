{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parser": "babel-parser",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "env": {
    "browser": false,
    "node": true
  },

  "ignorePatterns": ["node_modules/"],
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
